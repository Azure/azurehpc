{
  "location": "variables.location",
  "resource_group": "variables.resource_group",
  "admin_user": "variables.admin_user",
  "variables": {
    "location": "<NOT-SET>",
    "resource_group": "<NOT-SET>",
    "key_vault": "<NOT-SET>",
    "admin_user": "hpcadmin",
    "cc_password_secret_name": "CycleAdminPassword",
    "cc_image": "azurecyclecloud:azure-cyclecloud:cyclecloud-81:8.1.0"
  },
  "vnet": {
  },
  "resources": {},
  "install": [
    {
      "type": "local_script",
      "script": "create_keyvault.sh",
      "args": [
        "variables.resource_group",
        "variables.key_vault"
      ]
    },
    {
      "type": "local_script",
      "script": "create_password_in_keyvault.sh",
      "args": [
        "variables.key_vault",
        "variables.cc_password_secret_name"
      ]
    },
    {
      "type": "local_script",
      "script": "az_image_accept_terms.sh",
      "args": [
        "variables.cc_image"
      ]
    }
  ]
}